{
	"func":{
		"prefix": "func",
		"body":[
			"func ${1:関数名}(${2:引数}) {",
			"\t$3",
			"}"
		],
		"description": "関数の定義"
	},
	"main":{
		"prefix": "func main()",
		"body":[
			"func main(${1:引数}) {",
			"\t$2",
			"}"
		],
		"description": "wmsはこの関数から始まる"
	},
	"print":{
		"prefix": "print()",
		"body":[
			"print(${1:表示するもの});"
		],
		"description": "アラートを表示"
	},
	"for":{
		"prefix": "for()",
		"body":[
			"for(${1:変数名} in ${2:開始} .. ${3:終了}){",
			"\t$4",
			"}"
		],
		"description": "繰り返し"
	},
	"while":{
		"prefix": "while()",
		"body":[
			"while(${1:変数名} ${2|==,>=,<=,>,<,!=|} ${3:条件}){",
			"\t$4",
			"}"
		],
		"description": "条件を満たす間、繰り返す"
	},
	"if":{
		"prefix": "if()",
		"body":[
			"if(${1:変数名} ${2|==,>=,<=,>,<,!=|} ${3:条件}1){",
			"\t$4",
			"}"
		],
		"description": "分岐"
	},
	"else":{
		"prefix": "else",
		"body":[
			"else {",
			"\t$1",
			"}"
		],
		"description": "ifに当てはまらなかった時、実行される"
	},
	"break":{
		"prefix": "break",
		"body":[
			"break;"
		],
		"description": "forやwhileの処理を中断する"
	},
	"continue":{
		"prefix": "continue",
		"body":[
			"continue;"
		],
		"description": "forやwhileの現在の処理を飛ばし、次の繰り返しに進む"
	},
	"remove":{
		"prefix": "remove();",
		"body":[
			"remove(${1:変数名},${2:削除する要素});"
		],
		"description": "指定した配列の要素を削除する"
	},
	"size":{
		"prefix": "size();",
		"body":[
			"size(${1:配列名});"
		],
		"description": "指定した配列のサイズを取得する"
	},
